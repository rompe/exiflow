<Addin namespace="FSpot"
	name="ExiflowRunWithExigui"
	version="0.4.4.100"
	description="Open Exigui with all versions from selected photos( http://exiflow.org/ )."
	url="http://exiflow.org/"
	author="Sebastian Berthold"
	category="Tools">
	<Dependencies>
		<Addin id="Core" version="0.4.4.100"/>
	</Dependencies>
	<Extension path = "/FSpot/Menus/PhotoPopup">
		<Command id = "ExiflowRunWithExigui" _label = "Exiflow: Open with _Exigui" command_type = "ExiflowRunWithExiguiExtension.ExiflowRunWithExigui" insertbefore="OpenWith"/>
	</Extension>
</Addin>


